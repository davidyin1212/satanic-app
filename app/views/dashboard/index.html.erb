<div class="col-md-10 col-md-offset-1">
	<div class="col-md-4 col-md-offset-8" style="display: block, padding: 50px">
		<form action="<%= dashboard_index_path %>">
		  Jump to Date
		  <input type="date" name="date">
		  <input type="submit">
		</form>
	</div>
	<% @tweets.each do |tweet| %>
		<div class="col-md-4">
			<% if tweet.media.exists? %>
				<%= image_tag tweet.media.url(:medium) %>
			<% end %>
			<h1><%= tweet.post_name %></h1>
			<p><%= tweet.body %></p>
		</div>
	<% end %>
</div>